{
  "scripts": {
    "server": "node ./node_modules/http-server/bin/http-server -c-1 -p 8989",
    "start": "npm run server",
    "build": "node build.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js --color -c ./eslintrc.js \"src/**/*.js\" \"test/**/*.js\"",
    "unittest": "node ./node_modules/karma/bin/karma start karma.conf.js --single-run",
    "watch-test": "node ./node_modules/karma/bin/karma start karma.conf.js --watch",
    "coverage": "node ./node_modules/karma/bin/karma start karma.conf.js coverage --single-run",
    "test": "npm run lint && npm run unittest",
    "postinstall": "node ./node_modules/dependency-relocator/dependency-relocator.js ./src/third_party"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-minify": "^0.4.3",
    "babel-preset-minify": "^0.4.3",
    "dependency-relocator": "^0.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-jasmine": "^2.10.1",
    "fs-extra": "^6.0.1",
    "http-server": "^0.11.1",
    "jasmine": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-es6": "0.0.3",
    "karma-jasmine-html-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.32",
    "walk": "^2.3.14"
  },
  "dependencies": {
    "@webcomponents/html-imports": "git+https://git@github.com/webcomponents/html-imports.git",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "jed": "^1.1.1",
    "web-component-base-class": "^0.1.4"
  }
}
