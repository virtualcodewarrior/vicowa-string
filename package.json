{
  "name": "vicowa-web-components",
  "version": "v0.1.60",
  "description": "Various web components",
  "keywords": [
    "webComponents",
    "translatable",
    "validation",
    "collapsible"
  ],
  "homepage": "https://github.com/virtualcodearrior/vicowa-web-components",
  "bugs": {
    "url": "https://github.com/virtualcodewarrior/vicowa-web-components/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Rodney Draaisma"
  },
  "scripts": {
    "server": "node ./node_modules/http-server/bin/http-server -c-1 -p 8989",
    "start": "npm run server",
    "build": "node ./build.js",
    "doc": "node ./node_modules/jsdoc/jsdoc.js src -r -d doc -c jsdoc.conf.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js --color -c ./eslintrc.js \"src/**/*.js\" \"test/**/*.js\"",
    "unittest": "node ./node_modules/karma/bin/karma start karma.conf.js --single-run",
    "watch-test": "node ./node_modules/karma/bin/karma start karma.conf.js --watch",
    "coverage": "node ./node_modules/karma/bin/karma start karma.conf.js coverage --single-run",
    "test": "npm run lint && npm run unittest",
    "third_party": "node ./node_modules/dependency-relocator/dependency-relocator.js ./src/third_party",
    "prepare": "npm run build -- --npm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualcodewarrior/vicowa-web-components"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "files": [
    "source",
    "dist"
  ],
  "devDependencies": {
    "babel-minify": "^0.5.0",
    "babel-preset-minify": "^0.5.0",
    "dependency-relocator": "^0.1.2",
    "eslint": "^6.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "fs-extra": "^8.1.0",
    "http-server": "^0.11.1",
    "jasmine": "^3.4.0",
    "jsdoc": "^3.6.3",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-es6": "0.0.3",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "walk": "^2.3.14"
  },
  "dependencies": {
    "babylonjs": "^4.0.3",
    "babylonjs-loaders": "^4.0.3",
    "earcut": "^2.1.5",
    "intersection-observer": "^0.7.0",
    "jed": "^1.1.1",
    "lodash": "^4.17.15",
    "u2f-api": "^1.1.1",
    "web-component-base-class": "^1.0.1"
  }
}
